{
  "name": "lforms-fhir-app",
  "version": "0.14.10",
  "description": "A SMART on FHIR app for lforms",
  "repository": "https://github.com/lhncbc/lforms-fhir-app",
  "devDependencies": {
    "bower": "^1.8.8",
    "grunt": "^1.1.0",
    "grunt-angular-templates": "^1.2.0",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "git+https://github.com/yuriy-sedinkin/grunt-contrib-cssmin.git#1061f66987c1ae519201f7094add48cc286e41a7",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-injector": "^1.1.0",
    "grunt-ng-annotate": "^3.0.0",
    "grunt-rev": "^0.1.0",
    "grunt-shell": "^2.1.0",
    "grunt-usemin": "^3.1.1",
    "grunt-wiredep": "^3.0.1",
    "http-server": "^0.12.1",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jit-grunt": "^0.10.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "^5.4.3",
    "time-grunt": "^1.4.0",
    "tmp": "0.0.33"
  },
  "scripts": {
    "postinstall": "bower install && npm run update-webdriver",
    "update-deps": "npm update",
    "postupdate-deps": "bower update",
    "copy-npm-scripts": "cp node_modules/lforms-updater/browser/updater.js app/lforms-updater.js",
    "build": "npm run copy-npm-scripts && grunt build",
    "start": "echo 'Website starting at http://localhost:8000/lforms-fhir-app'; http-server -p 8000 -c-1 .",
    "start-dist": "http-server -p 8000 -c-1 dist",
    "update-webdriver": "webdriver-manager update --versions.standalone 3.141.59 # skip broken 4.0.0-alpha version",
    "test": "protractor e2e-tests/protractor.conf.js"
  },
  "dependencies": {
    "lforms-updater": "^23.0.2"
  }
}
