{
  "name": "lforms-fhir-app",
  "version": "1.3.1",
  "description": "A SMART on FHIR app for lforms",
  "repository": "https://github.com/lhncbc/lforms-fhir-app",
  "source": [
    ".tmp/index.html",
    ".tmp/launch.html"
  ],
  "serverPort": "8000",
  "devDependencies": {
    "bower": "^1.8.8",
    "grunt": "^1.1.0",
    "grunt-angular-templates": "^1.2.0",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "git+https://github.com/yuriy-sedinkin/grunt-contrib-cssmin.git#1061f66987c1ae519201f7094add48cc286e41a7",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-injector": "^1.1.0",
    "grunt-ng-annotate": "^3.0.0",
    "grunt-rev": "^0.1.0",
    "grunt-shell": "^2.1.0",
    "grunt-usemin": "^3.1.1",
    "grunt-wiredep": "^3.0.1",
    "http-server": "^0.12.1",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jit-grunt": "^0.10.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "parcel": "^2.0.1",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "ssi": "^0.3.0",
    "superagent": "^6.1.0",
    "time-grunt": "^1.4.0",
    "tmp": "0.0.33",
    "uglify-js": "3.9.1"
  },
  "old_scripts": {
    "postinstall": "bower install && npm run update-webdriver",
    "update-deps": "npm update",
    "postupdate-deps": "bower update",
    "cp-lforms-updater": "mkdir -p app/lforms-updater; cp node_modules/lforms-updater/browser/updater.js* app/lforms-updater/",
    "cp-fhirclient": "cp node_modules/fhirclient/build/fhir-client.js app",
    "copy-npm-scripts": "npm run cp-lforms-updater && npm run cp-fhirclient",
    "test-dist": "npm run start-dist & sleep 5; protractor e2e-tests/protractor.conf.js; s=$?; kill %1; exit $s"
  },
  "scripts": {
    "build:clean": "rimraf dist .tmp",
    "build:pages": "node -e '(new (require(\"ssi\"))(\"source/html-ssi\", \".tmp\", \"/*.html\", true)).compile()'",
    "build:preparcel": "npm run build:clean && npm run build:pages",
    "build-dev": "npm run build:preparcel && parcel build --dist-dir dist/$npm_package_name --public-url . --no-optimize",
    "build:parcel:comment": "PARCEL_WORKER_BACKEND=process works around a seg fault issue, which is fixed on the master branch but we are waiting for a new version",
    "build:parcel": "PARCEL_WORKER_BACKEND=process parcel build --dist-dir dist/$npm_package_name --public-url .",
    "build": "npm run build:preparcel && npm run build:parcel",
    "prestart": "echo \"Website starting at http://localhost:8000/$npm_package_name\"",
    "start": "http-server -s -o $npm_package_name -a localhost -p $npm_package_serverPort -c-1 dist",
    "start-public": "http-server -s -o $npm_package_name -p $npm_package_serverPort -c-1 dist",
    "prestart-public": "npm run prestart",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update --versions.chrome $(google-chrome --version |awk '{print $NF}')",
    "test": "npm run start & sleep 5; protractor e2e-tests/protractor.conf.js; s=$?; kill %1; exit $s"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "fhirclient": "^2.3.11",
    "lforms-updater": "^29.0.2"
  }
}
